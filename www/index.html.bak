    .menu-card
    {    text-align: right;
    padding-right: 15px;
    position: absolute;
    right: 0;
    bottom: 100px;
    top:0;
        }
    .menu-card h3
    { 
    font-weight: 300;
    font-size: 16px;
    text-align: center;
    padding: 6px 15px;
    height: 36px;
    margin: 9px auto;
    color: #03AEFF;
    text-transform: uppercase;
    border-radius: 2px;
    border: 1px solid rgba(3, 174, 255, 0.53);   
    }
    .menu-card h3.activated
    { background: #03AEFF;
      color:#fff;
        }
    .menu-item .item .icon{ color:#11c1f3}    
    
     .tracker-panel{position: absolute;bottom: 0;margin: 0;left: 0;right: 0; height:60px}

 
 .tracker-panel.open
 {
     height:200px;
     background:#fff;
     border-top:1px solid #eee;
     padding-bottom: 64px;
    padding-top: 3px;
   }
   .tracker-panel.open .tracker_inside
   {    overflow-y: auto;
    position: absolute;
    top: 0;
     overflow-x: hidden;
    background: #fff;
    left: 0px;
    right: 0px;
    bottom: 60px;
       
       }
        
